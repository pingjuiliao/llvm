

##################################
#####   receive_until
####################################
  %4 = alloca i8*, align 8
  %5 = alloca i8, align 1
  %6 = alloca i64, align 8
  %7 = alloca i64, align 8
  %8 = alloca i64, align 8
  %9 = alloca i8, align 1
  store i8* %0, i8** %4, align 8
  store i8 %1, i8* %5, align 1
  store i64 %2, i64* %6, align 8
  store i64 0, i64* %7, align 8
  store i64 0, i64* %8, align 8
  store i8 0, i8* %9, align 1
  br label %10
  %11 = load i64, i64* %7, align 8
  %12 = load i64, i64* %6, align 8
  %13 = icmp ult i64 %11, %12
  br i1 %13, label %14, label %35
  %15 = load i8*, i8** %4, align 8
  %16 = load i64, i64* %7, align 8
  %17 = getelementptr inbounds i8, i8* %15, i64 %16
  store i8 0, i8* %17, align 1
  %18 = call i64 @read(i32 0, i8* %9, i64 1)
  store i64 %18, i64* %8, align 8
  %19 = icmp eq i64 %18, 0
  br i1 %19, label %20, label %21
  store i64 0, i64* %7, align 8
  br label %35
  %22 = load i8, i8* %9, align 1
  %23 = sext i8 %22 to i32
  %24 = load i8, i8* %5, align 1
  %25 = sext i8 %24 to i32
  %26 = icmp eq i32 %23, %25
  br i1 %26, label %27, label %28
  br label %35
  %29 = load i8, i8* %9, align 1
  %30 = load i8*, i8** %4, align 8
  %31 = load i64, i64* %7, align 8
  %32 = getelementptr inbounds i8, i8* %30, i64 %31
  store i8 %29, i8* %32, align 1
  %33 = load i64, i64* %7, align 8
  %34 = add i64 %33, 1
  store i64 %34, i64* %7, align 8
  br label %10
  %36 = load i64, i64* %7, align 8
  ret i64 %36


##################################
#####   print_message
####################################
  %3 = alloca i32, align 4
  %4 = alloca i32, align 4
  store i32 %0, i32* %3, align 4
  store i32 %1, i32* %4, align 4
  %5 = load i32, i32* %3, align 4
  %6 = sext i32 %5 to i64
  %7 = getelementptr inbounds [10 x [20 x %struct.Message]], [10 x [20 x %struct.Message]]* getelementptr inbounds (%struct.GLOBALS, %struct.GLOBALS* @globs, i32 0, i32 0), i64 0, i64 %6
  %8 = load i32, i32* %4, align 4
  %9 = sext i32 %8 to i64
  %10 = getelementptr inbounds [20 x %struct.Message], [20 x %struct.Message]* %7, i64 0, i64 %9
  %11 = getelementptr inbounds %struct.Message, %struct.Message* %10, i32 0, i32 1
  %12 = getelementptr inbounds [15 x i8], [15 x i8]* %11, i32 0, i32 0
  %13 = call i32 @strcmp(i8* %12, i8* getelementptr inbounds ([1 x i8], [1 x i8]* @.str, i32 0, i32 0)) #6
  %14 = icmp ne i32 %13, 0
  br i1 %14, label %16, label %15
  br label %43
  %17 = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([20 x i8], [20 x i8]* @.str.1, i32 0, i32 0))
  %18 = load i32, i32* %3, align 4
  %19 = sext i32 %18 to i64
  %20 = getelementptr inbounds [10 x [20 x %struct.Message]], [10 x [20 x %struct.Message]]* getelementptr inbounds (%struct.GLOBALS, %struct.GLOBALS* @globs, i32 0, i32 0), i64 0, i64 %19
  %21 = load i32, i32* %4, align 4
  %22 = sext i32 %21 to i64
  %23 = getelementptr inbounds [20 x %struct.Message], [20 x %struct.Message]* %20, i64 0, i64 %22
  %24 = getelementptr inbounds %struct.Message, %struct.Message* %23, i32 0, i32 1
  %25 = getelementptr inbounds [15 x i8], [15 x i8]* %24, i32 0, i32 0
  %26 = load i32, i32* %3, align 4
  %27 = sext i32 %26 to i64
  %28 = getelementptr inbounds [10 x [20 x %struct.Message]], [10 x [20 x %struct.Message]]* getelementptr inbounds (%struct.GLOBALS, %struct.GLOBALS* @globs, i32 0, i32 0), i64 0, i64 %27
  %29 = load i32, i32* %4, align 4
  %30 = sext i32 %29 to i64
  %31 = getelementptr inbounds [20 x %struct.Message], [20 x %struct.Message]* %28, i64 0, i64 %30
  %32 = getelementptr inbounds %struct.Message, %struct.Message* %31, i32 0, i32 2
  %33 = getelementptr inbounds [15 x i8], [15 x i8]* %32, i32 0, i32 0
  %34 = load i32, i32* %3, align 4
  %35 = sext i32 %34 to i64
  %36 = getelementptr inbounds [10 x [20 x %struct.Message]], [10 x [20 x %struct.Message]]* getelementptr inbounds (%struct.GLOBALS, %struct.GLOBALS* @globs, i32 0, i32 0), i64 0, i64 %35
  %37 = load i32, i32* %4, align 4
  %38 = sext i32 %37 to i64
  %39 = getelementptr inbounds [20 x %struct.Message], [20 x %struct.Message]* %36, i64 0, i64 %38
  %40 = getelementptr inbounds %struct.Message, %struct.Message* %39, i32 0, i32 3
  %41 = getelementptr inbounds [15 x i8], [15 x i8]* %40, i32 0, i32 0
  %42 = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([26 x i8], [26 x i8]* @.str.2, i32 0, i32 0), i8* %25, i8* %33, i8* %41)
  br label %43
  ret void


##################################
#####   add_message
####################################
  %5 = alloca i32, align 4
  %6 = alloca i8*, align 8
  %7 = alloca i8*, align 8
  %8 = alloca i8*, align 8
  %9 = alloca i8, align 1
  %10 = alloca i8, align 1
  %11 = alloca i32, align 4
  %12 = alloca i32, align 4
  %13 = alloca i32, align 4
  %14 = alloca i32, align 4
  %15 = alloca %struct.Message*, align 8
  store i8* %0, i8** %6, align 8
  store i8* %1, i8** %7, align 8
  store i8* %2, i8** %8, align 8
  store i8 %3, i8* %9, align 1
  store void (...)* bitcast (void (i32, i32)* @print_message to void (...)*), void (...)** getelementptr inbounds (%struct.GLOBALS, %struct.GLOBALS* @globs, i32 0, i32 1), align 8
  store i8 -1, i8* %10, align 1
  store i32 0, i32* %11, align 4
  br label %16
  %17 = load i32, i32* %11, align 4
  %18 = load i32, i32* @user_count, align 4
  %19 = icmp slt i32 %17, %18
  br i1 %19, label %20, label %37
  %21 = load i8*, i8** %6, align 8
  %22 = load i32, i32* %11, align 4
  %23 = sext i32 %22 to i64
  %24 = getelementptr inbounds [10 x %struct.User*], [10 x %struct.User*]* @listOfUsers, i64 0, i64 %23
  %25 = load %struct.User*, %struct.User** %24, align 8
  %26 = getelementptr inbounds %struct.User, %struct.User* %25, i32 0, i32 0
  %27 = getelementptr inbounds [15 x i8], [15 x i8]* %26, i32 0, i32 0
  %28 = call i32 @strcmp(i8* %21, i8* %27) #6
  %29 = icmp ne i32 %28, 0
  br i1 %29, label %33, label %30
  %31 = load i32, i32* %11, align 4
  %32 = trunc i32 %31 to i8
  store i8 %32, i8* %10, align 1
  br label %37
  br label %34
  %35 = load i32, i32* %11, align 4
  %36 = add nsw i32 %35, 1
  store i32 %36, i32* %11, align 4
  br label %16
  %38 = load i8, i8* %10, align 1
  %39 = sext i8 %38 to i32
  %40 = icmp eq i32 %39, -1
  br i1 %40, label %41, label %43
  %42 = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([22 x i8], [22 x i8]* @.str.3, i32 0, i32 0))
  store i32 0, i32* %5, align 4
  br label %128
  store i32 -1, i32* %12, align 4
  store i32 -1, i32* %13, align 4
  %44 = load i8, i8* %9, align 1
  %45 = sext i8 %44 to i32
  %46 = icmp eq i32 %45, 1
  br i1 %46, label %47, label %50
  %48 = load i8*, i8** %7, align 8
  %49 = call i32 @get_user__index(i8* %48)
  store i32 %49, i32* %13, align 4
  br label %53
  %51 = load i8*, i8** %6, align 8
  %52 = call i32 @get_user__index(i8* %51)
  store i32 %52, i32* %13, align 4
  br label %53
  %54 = load %struct.User*, %struct.User** @current_user, align 8
  %55 = getelementptr inbounds %struct.User, %struct.User* %54, i32 0, i32 3
  %56 = load i32, i32* %55, align 4
  %57 = icmp eq i32 %56, 1
  br i1 %57, label %58, label %70
  %59 = load %struct.User*, %struct.User** @current_user, align 8
  %60 = getelementptr inbounds %struct.User, %struct.User* %59, i32 0, i32 3
  store i32 0, i32* %60, align 4
  %61 = load %struct.User*, %struct.User** @current_user, align 8
  %62 = getelementptr inbounds %struct.User, %struct.User* %61, i32 0, i32 4
  %63 = load i32, i32* %62, align 4
  %64 = icmp sge i32 %63, 20
  br i1 %64, label %65, label %69
  %66 = load %struct.User*, %struct.User** @current_user, align 8
  %67 = getelementptr inbounds %struct.User, %struct.User* %66, i32 0, i32 4
  %68 = load i32, i32* %67, align 4
  store i32 %68, i32* %12, align 4
  br label %91
  br label %70
  store i32 0, i32* %14, align 4
  br label %71
  %72 = load i32, i32* %14, align 4
  %73 = icmp slt i32 %72, 21
  br i1 %73, label %74, label %90
  %75 = load i32, i32* %13, align 4
  %76 = sext i32 %75 to i64
  %77 = getelementptr inbounds [10 x [20 x %struct.Message]], [10 x [20 x %struct.Message]]* getelementptr inbounds (%struct.GLOBALS, %struct.GLOBALS* @globs, i32 0, i32 0), i64 0, i64 %76
  %78 = load i32, i32* %14, align 4
  %79 = sext i32 %78 to i64
  %80 = getelementptr inbounds [20 x %struct.Message], [20 x %struct.Message]* %77, i64 0, i64 %79
  %81 = getelementptr inbounds %struct.Message, %struct.Message* %80, i32 0, i32 0
  %82 = load i32, i32* %81, align 4
  %83 = icmp ne i32 %82, 48879
  br i1 %83, label %84, label %86
  %85 = load i32, i32* %14, align 4
  store i32 %85, i32* %12, align 4
  br label %91
  br label %87
  %88 = load i32, i32* %14, align 4
  %89 = add nsw i32 %88, 1
  store i32 %89, i32* %14, align 4
  br label %71
  br label %91
  %92 = load i32, i32* %12, align 4
  %93 = icmp eq i32 %92, -1
  br i1 %93, label %94, label %95
  store i32 0, i32* %5, align 4
  br label %128
  %96 = load i32, i32* %13, align 4
  %97 = sext i32 %96 to i64
  %98 = getelementptr inbounds [10 x [20 x %struct.Message]], [10 x [20 x %struct.Message]]* getelementptr inbounds (%struct.GLOBALS, %struct.GLOBALS* @globs, i32 0, i32 0), i64 0, i64 %97
  %99 = load i32, i32* %12, align 4
  %100 = sext i32 %99 to i64
  %101 = getelementptr inbounds [20 x %struct.Message], [20 x %struct.Message]* %98, i64 0, i64 %100
  store %struct.Message* %101, %struct.Message** %15, align 8
  %102 = load %struct.Message*, %struct.Message** %15, align 8
  %103 = getelementptr inbounds %struct.Message, %struct.Message* %102, i32 0, i32 0
  store i32 48879, i32* %103, align 4
  %104 = load %struct.Message*, %struct.Message** %15, align 8
  %105 = getelementptr inbounds %struct.Message, %struct.Message* %104, i32 0, i32 2
  %106 = getelementptr inbounds [15 x i8], [15 x i8]* %105, i32 0, i32 0
  %107 = load i8*, i8** %7, align 8
  %108 = call i8* @strcpy(i8* %106, i8* %107) #7
  %109 = load %struct.Message*, %struct.Message** %15, align 8
  %110 = getelementptr inbounds %struct.Message, %struct.Message* %109, i32 0, i32 1
  %111 = getelementptr inbounds [15 x i8], [15 x i8]* %110, i32 0, i32 0
  %112 = load i8*, i8** %6, align 8
  %113 = call i8* @strcpy(i8* %111, i8* %112) #7
  %114 = load %struct.Message*, %struct.Message** %15, align 8
  %115 = getelementptr inbounds %struct.Message, %struct.Message* %114, i32 0, i32 3
  %116 = getelementptr inbounds [15 x i8], [15 x i8]* %115, i32 0, i32 0
  %117 = load i8*, i8** %8, align 8
  %118 = call i8* @strcpy(i8* %116, i8* %117) #7
  %119 = load i8, i8* %9, align 1
  %120 = load %struct.Message*, %struct.Message** %15, align 8
  %121 = getelementptr inbounds %struct.Message, %struct.Message* %120, i32 0, i32 4
  store i8 %119, i8* %121, align 1
  %122 = load i32, i32* @msg_count_login, align 4
  %123 = add nsw i32 %122, 1
  store i32 %123, i32* @msg_count_login, align 4
  %124 = load %struct.User*, %struct.User** @current_user, align 8
  %125 = getelementptr inbounds %struct.User, %struct.User* %124, i32 0, i32 4
  %126 = load i32, i32* %125, align 4
  %127 = add nsw i32 %126, 1
  store i32 %127, i32* %125, align 4
  store i32 1, i32* %5, align 4
  br label %128
  %129 = load i32, i32* %5, align 4
  ret i32 %129


##################################
#####   get_user__index
####################################
  %2 = alloca i32, align 4
  %3 = alloca i8*, align 8
  %4 = alloca i32, align 4
  store i8* %0, i8** %3, align 8
  store i32 0, i32* %4, align 4
  br label %5
  %6 = load i32, i32* %4, align 4
  %7 = load i32, i32* @user_count, align 4
  %8 = icmp slt i32 %6, %7
  br i1 %8, label %9, label %25
  %10 = load i8*, i8** %3, align 8
  %11 = load i32, i32* %4, align 4
  %12 = sext i32 %11 to i64
  %13 = getelementptr inbounds [10 x %struct.User*], [10 x %struct.User*]* @listOfUsers, i64 0, i64 %12
  %14 = load %struct.User*, %struct.User** %13, align 8
  %15 = getelementptr inbounds %struct.User, %struct.User* %14, i32 0, i32 0
  %16 = getelementptr inbounds [15 x i8], [15 x i8]* %15, i32 0, i32 0
  %17 = call i32 @strcmp(i8* %10, i8* %16) #6
  %18 = icmp eq i32 %17, 0
  br i1 %18, label %19, label %21
  %20 = load i32, i32* %4, align 4
  store i32 %20, i32* %2, align 4
  br label %26
  br label %22
  %23 = load i32, i32* %4, align 4
  %24 = add nsw i32 %23, 1
  store i32 %24, i32* %4, align 4
  br label %5
  store i32 -1, i32* %2, align 4
  br label %26
  %27 = load i32, i32* %2, align 4
  ret i32 %27


##################################
#####   list_drafts
####################################
  %2 = alloca i8*, align 8
  %3 = alloca i32, align 4
  %4 = alloca i32, align 4
  %5 = alloca i32, align 4
  store i8* %0, i8** %2, align 8
  %6 = load i8*, i8** %2, align 8
  %7 = call i32 @get_user__index(i8* %6)
  store i32 %7, i32* %3, align 4
  store i32 0, i32* %4, align 4
  store i32 0, i32* %5, align 4
  br label %8
  %9 = load i32, i32* %5, align 4
  %10 = icmp slt i32 %9, 20
  br i1 %10, label %11, label %54
  %12 = load i32, i32* %3, align 4
  %13 = sext i32 %12 to i64
  %14 = getelementptr inbounds [10 x [20 x %struct.Message]], [10 x [20 x %struct.Message]]* getelementptr inbounds (%struct.GLOBALS, %struct.GLOBALS* @globs, i32 0, i32 0), i64 0, i64 %13
  %15 = load i32, i32* %5, align 4
  %16 = sext i32 %15 to i64
  %17 = getelementptr inbounds [20 x %struct.Message], [20 x %struct.Message]* %14, i64 0, i64 %16
  %18 = getelementptr inbounds %struct.Message, %struct.Message* %17, i32 0, i32 0
  %19 = load i32, i32* %18, align 4
  %20 = icmp ne i32 %19, 48879
  br i1 %20, label %21, label %22
  br label %51
  %23 = load i8*, i8** %2, align 8
  %24 = load i32, i32* %3, align 4
  %25 = sext i32 %24 to i64
  %26 = getelementptr inbounds [10 x [20 x %struct.Message]], [10 x [20 x %struct.Message]]* getelementptr inbounds (%struct.GLOBALS, %struct.GLOBALS* @globs, i32 0, i32 0), i64 0, i64 %25
  %27 = load i32, i32* %5, align 4
  %28 = sext i32 %27 to i64
  %29 = getelementptr inbounds [20 x %struct.Message], [20 x %struct.Message]* %26, i64 0, i64 %28
  %30 = getelementptr inbounds %struct.Message, %struct.Message* %29, i32 0, i32 2
  %31 = getelementptr inbounds [15 x i8], [15 x i8]* %30, i32 0, i32 0
  %32 = call i32 @strcmp(i8* %23, i8* %31) #6
  %33 = icmp ne i32 %32, 0
  br i1 %33, label %50, label %34
  %35 = load i32, i32* %3, align 4
  %36 = sext i32 %35 to i64
  %37 = getelementptr inbounds [10 x [20 x %struct.Message]], [10 x [20 x %struct.Message]]* getelementptr inbounds (%struct.GLOBALS, %struct.GLOBALS* @globs, i32 0, i32 0), i64 0, i64 %36
  %38 = load i32, i32* %5, align 4
  %39 = sext i32 %38 to i64
  %40 = getelementptr inbounds [20 x %struct.Message], [20 x %struct.Message]* %37, i64 0, i64 %39
  %41 = getelementptr inbounds %struct.Message, %struct.Message* %40, i32 0, i32 4
  %42 = load i8, i8* %41, align 1
  %43 = sext i8 %42 to i32
  %44 = icmp eq i32 %43, 1
  br i1 %44, label %45, label %50
  store i32 1, i32* %4, align 4
  %46 = load void (...)*, void (...)** getelementptr inbounds (%struct.GLOBALS, %struct.GLOBALS* @globs, i32 0, i32 1), align 8
  %47 = load i32, i32* %3, align 4
  %48 = load i32, i32* %5, align 4
  %49 = bitcast void (...)* %46 to void (i32, i32, ...)*
  call void (i32, i32, ...) %49(i32 %47, i32 %48)
  br label %50
  br label %51
  %52 = load i32, i32* %5, align 4
  %53 = add nsw i32 %52, 1
  store i32 %53, i32* %5, align 4
  br label %8
  %55 = load i32, i32* %4, align 4
  %56 = icmp eq i32 %55, 0
  br i1 %56, label %57, label %59
  %58 = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([26 x i8], [26 x i8]* @.str.4, i32 0, i32 0))
  br label %59
  ret void


