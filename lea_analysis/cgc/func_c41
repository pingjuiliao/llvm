##################################
#####   main
####################################
  %1 = alloca i32, align 4
  store i32 0, i32* %1, align 4
  store i32 0, i32* @msg_count_login, align 4
  store i32 0, i32* @user_count, align 4
  store i32 0, i32* @bad_login_count, align 4
  br label %2
  br label %3
  call void @print_A()
  %4 = call i32 @handle_A_input()
  %5 = icmp eq i32 %4, 2
  br i1 %5, label %6, label %7
  br label %8
  br label %3
  br label %9
  call void @print_B()
  %10 = call i32 @handle_B_input()
  %11 = icmp eq i32 %10, 2
  br i1 %11, label %12, label %13
  br label %2
  br label %9
  %15 = load i32, i32* %1, align 4
  ret i32 %15


